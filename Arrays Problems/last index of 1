int lastIndex(string s) 
    {
        
        int i = 0;
        int ans = 0;
        bool flag = 0;
        while(i < s.length()){
            if(s[i] == '1'){
                ans = i;
                flag = 1;
            }
            i++;
        }
        
       if(flag){
           return ans;
       } else {
           return -1;
       }
                
    }
